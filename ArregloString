#Eclipse Java
import java.util.Scanner;


public class ArregloString {

	public static void main(String[] args)
	{
		String[] arregloString = new String[10];
		
		Scanner teclado = new Scanner(System.in);
		
		for (int i = 0; i < arregloString.length; i++) {
			System.out.println("Ingrese Nombre "+(i+1));
			arregloString[i] = teclado.next();
		}
		
		for (int i = 0; i < arregloString.length; i++) {
			System.out.println("Nombre "+(i+1)+": "+arregloString[i]);
		}
			
	}
}
